{
    "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "jsx": "react-jsx",
        "lib": ["es2017"],
        "moduleResolution": "node",
        "noEmit": true,
        "noEmitOnError": true,
        "strict": true,
        "target": "esnext",
        "baseUrl": "."
    },
}